<Card Title="Nouvelle personne">
    <CardBody>
        <InlineForm>
            <FormGroup Label="Nom">
                <input type="text" class="form-control" placeholder="Nom"/>
            </FormGroup>
            <FormGroup Label="Prénom">
                <input type="text" class="form-control" placeholder="Prénom" />
            </FormGroup>
            <FormGroup Label="Téléphone">
                <input type="text" class="form-control" placeholder="N° de téléphone" />
            </FormGroup>
            <FormGroup Label="Adresse mail">
                <input type="email" class="form-control" placeholder="Adresse mail" />
            </FormGroup>
        </InlineForm>
    </CardBody>
    <CardFooter>
        <SubmitOrCancel CancelLabel="Annuler" SubmitLabel="Enregister"></SubmitOrCancel>
    </CardFooter>
</Card>


